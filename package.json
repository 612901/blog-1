{
  "name": "xlenco-blog",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server",
    "yuque": "yuque-hexo sync && npm run build && hexo algolia && gulp"
  },
  "yuqueConfig": {
    "postPath": "/source/_posts/yuque",
    "cachePath": "yuque.json",
    "lastGeneratePath": "time.log",
    "mdNameFormat": "title",
    "adapter": "markdown",
    "concurrency": "5",
    "baseUrl": "https://www.yuque.com/api/v2",
    "login": "xlenco",
    "repo": "posts",
    "onlyPublished": true,
    "onlyPublic": false
  },
  "hexo": {
    "version": "6.3.0"
  },
  "dependencies": {
    "gulp": "^4.0.2",
    "hexo": "^6.3.0",
    "hexo-abbrlink": "^2.2.1",
    "hexo-algoliasearch": "^1.0.0",
    "hexo-butterfly-tag-plugins-plus": "^1.0.17",
    "hexo-deployer-git": "^3.0.0",
    "hexo-filter-nofollow": "^2.0.2",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-sitemap": "^3.0.1",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-markdown-it-plus": "^1.0.6",
    "hexo-renderer-pug": "^3.0.0",
    "hexo-renderer-stylus": "^2.1.0",
    "hexo-server": "^3.0.0",
    "hexo-wordcount": "^6.0.1",
    "hexo-filter-fcircle-anzhiyu": "^1.1.5",
    "yuque-hexo": "^1.9.5",
    "hexo-yam": "^8.0.0"
  },
  "devDependencies": {
    "gulp-clean-css": "^4.3.0",
    "gulp-html-minifier-terser": "^7.1.0",
    "gulp-htmlclean": "^2.7.22",
    "gulp-terser": "^2.1.0"
  }
}
